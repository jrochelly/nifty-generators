<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <title><%= Rails.application.class.parent %></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <%%= stylesheet_link_tag    "<%= file_name %>", media: "all", "data-turbolinks-track" => true %>
    <%%= javascript_include_tag "<%= file_name %>", "data-turbolinks-track" => true %>
    <%%= csrf_meta_tags %>
    <%%= yield(:head) %>
</head>
<body>
  <% if layout_name == "application" %>
  <header class="bg-primary">
    <div class="logo"><%%= link_to "<%= Rails.application.class.parent %>", '/' %> </div>
    <nav>
      <li></li>
    </nav>
  </header>
  <% end %>
  <%% if notice %>
    <div class="container">
      <div class="alert alert-info col-md-6 col-md-offset-3"><%%= notice %></div>
    </div>
  <%% end %>
  <div class="container">
    <%%= yield %>
  </div>
  <footer>
    <%%= yield(:foot) %>
    <script>
       (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
       (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
       m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
       })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

       ga('create', 'UA-XXXXX-X');
       ga('send', 'pageview');
    </script>

      <!--[if lt IE 9]>
      <script src="bower_components/es5-shim/es5-shim.js"></script>
      <script src="bower_components/json3/lib/json3.min.js"></script>
      <![endif]-->
  </footer>
</body>
</html>